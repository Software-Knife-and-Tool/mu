(mu:vector-len #(:t 1 a -3))
(mu:svref #(:t 1 a -3) 1)
(mu:vector-type #(:byte 1 2 3))
(mu:vector-len #(:byte 1 2 3))
(mu:svref #(:byte 1 2 3) 1)
(mu:vector-type #(:char #\1 #\2 #\3))
(mu:vector-len #(:char #\1 #\2 #\3))
(mu:svref #(:char #\1 #\2 #\3) 1)
(mu:vector-len #(:fixnum 1 2 -3))
(mu:svref #(:fixnum 1 2 -3) 1)
(mu:vector-type #(:fixnum 1 2 -3))
(mu:vector-len #(:float 1.0 2.0 -3.0))
(mu:svref #(:float 1.0 2.0 -3.0) 1)
(mu:vector-type #(:float 1.0 2.0 -3.0))
(mu:make-vector :byte '(1 2 3))
(mu:make-vector :t '(1 2 3))
(mu:make-vector :fixnum '(1 2 3))
(mu:make-vector :float '(1.0 2.0 -3.0))
