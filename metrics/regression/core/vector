(core:sv-type #(:byte 1 2 3))
(core:sv-len #(:byte 1 2 3))
(core:sv-ref #(:byte 1 2 3) 1)
(core:sv-type #(:char #\1 #\2 #\3))
(core:sv-len #(:char #\1 #\2 #\3))
(core:sv-ref #(:char #\1 #\2 #\3) 1)
(core:sv-len #(:fixnum 1 2 -3))
(core:sv-ref #(:fixnum 1 2 -3) 1)
(core:sv-type #(:fixnum 1 2 -3))
(core:sv-len #(:float 1.0 2.0 -3.0))
(core:sv-ref #(:float 1.0 2.0 -3.0) 1)
(core:sv-type #(:float 1.0 2.0 -3.0))
(core:vector :byte '(1 2 3))
(core:vector :t '(1 2 3))
