(mu:boundp 'mu:foo)	:nil
(mu:boundp 'mu:*standard-input*)	mu:*standard-input*
(mu:boundp :foo)	:t
(mu:boundp ())	:t
(mu:symbol-name 'mu:*standard-input*)	"*standard-input*"
(mu:symbol-namespace 'mu:*standard-input*)	#s(:ns #(:t 1 mu))
(mu:symbol-namespace 'std-in)	#s(:ns #(:t 0 ))
(mu:symbol-namespace :std-in)	#s(:ns #(:t 2 keyword))
(mu:symbol-namespace ())	#s(:ns #(:t 0 ))
(mu:symbol-namespace :nil)	#s(:ns #(:t 0 ))
(mu:symbol-value 'mu:*standard-input*)	#<stream: 0 :standard-input :input :open>
(mu:make-symbol "abcde")	#:abcde
