(core:type-of (:lambda ()))	:func
(:quote a)	a
((:lambda ()))	:nil
((:lambda () 1))	1
((:lambda (a) a) 1)	1
((:lambda (a) (core:sum a 2)) 1)	3
(:if :t :t :nil)	:t
(:if :nil :t :nil)	:nil
(:if 1 (core:sum 1 0) (core:sum 1 1))	1
(:if (core:eq 1 1) (core:sum 1 0) (core:sum 1 1))	1
(:if (core:eq 1 2) (core:sum 1 0) (core:sum 1 1))	2
