(core:vector-len "abcde")	5
(core:vector-len "abcdeghi")	8
(core:vector-len "abcdegh")	7
(core:vector-ref "abcde" 2)	#\c
(core:vector-type "abcde")	:char
(core:vector-len #(:t 1 a -3))	3
(core:vector-ref #(:t 1 a -3) 1)	a
(core:vector-type #(:byte 1 2 3))	:byte
(core:vector-len #(:byte 1 2 3))	3
(core:vector-ref #(:byte 1 2 3) 1)	2
(core:vector-type #(:char #\1 #\2 #\3))	:char
(core:vector-len #(:char #\1 #\2 #\3))	3
(core:vector-ref #(:char #\1 #\2 #\3) 1)	#\2
(core:vector-len #(:fixnum 1 2 -3))	3
(core:vector-ref #(:fixnum 1 2 -3) 1)	2
(core:vector-type #(:fixnum 1 2 -3))	:fixnum
(core:vector-len #(:float 1.0 2.0 -3.0))	3
(core:vector-ref #(:float 1.0 2.0 -3.0) 1)	2.0000
(core:vector-type #(:float 1.0 2.0 -3.0))	:float
(core:make-vector :byte '(1 2 3))	#(:byte 1 2 3)
(core:make-vector :t '(1 2 3))	#(:t 1 2 3)
(core:make-vector :fixnum '(1 2 3))	#(:fixnum 1 2 3)
(core:make-vector :float '(1.0 2.0 -3.0))	#(:float 1.0000 2.0000 -3.0000)
