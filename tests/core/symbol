(core:boundp 'core:foo)	:nil
(core:boundp 'core:standard-input)	core:standard-input
(core:symbol-name 'core:standard-input)	"standard-input"
(core:symbol-ns 'core:standard-input)	#<:ns "core">
(core:symbol-ns 'std-in)	#<:ns "">
(core:symbol-ns :std-in)	#<:ns "keyword">
(core:symbol-ns ())	#<:ns "">
(core:symbol-ns :nil)	#<:ns "">
(core:symbol-value 'core:standard-input)	#<stream: 0 :standard-input :input :open>
(core:make-symbol "abcde")	abcde
