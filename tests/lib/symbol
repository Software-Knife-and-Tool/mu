(lib:boundp 'lib:foo)	:nil
(lib:boundp 'lib:standard-input)	lib:standard-input
(lib:symbol-name 'lib:standard-input)	"standard-input"
(lib:symbol-ns 'lib:standard-input)	#<:ns "lib">
(lib:symbol-ns 'std-in)	#<:ns "">
(lib:symbol-ns :std-in)	#<:ns "keyword">
(lib:symbol-ns ())	#<:ns "">
(lib:symbol-ns :nil)	#<:ns "">
(lib:symbol-value 'lib:standard-input)	#<stream: 0 :standard-input :input :open>
(lib:make-symbol "abcde")	abcde
