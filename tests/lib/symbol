(lib:boundp 'lib:foo)	:nil
(lib:boundp 'lib:std-in)	lib:std-in
(lib:symbol-name 'lib:std-in)	"std-in"
(lib:symbol-ns 'lib:std-in)	#<:ns lib>
(lib:symbol-ns 'std-in)	#<:ns :nil>
(lib:symbol-ns :std-in)	#<:ns keyword>
(lib:symbol-ns ())	#<:ns :nil>
(lib:symbol-ns :nil)	#<:ns :nil>
(lib:symbol-value 'lib:std-in)	#<stream: id: 0 type: std-in dir: :input state: open>
(lib:make-symbol "abcde")	abcde
