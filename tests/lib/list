(lib:append '(1) 1)	(1 . 1)
(lib:append '(1) '(1))	(1 1)
(lib:car '(1))	1
(lib:car ())	:nil
(lib:cdr '(1 2))	(2)
(lib:cdr ())	:nil
(lib:cons 1 ())	(1)
(lib:cons 1 2)	(1 . 2)
(lib:length '(1))	1
(lib:length ())	0
(lib:nth 0 '(1 . 2))	1
(lib:nth 0 '(1 2))	1
(lib:nth 0 ())	:nil
(lib:nth 1 '(1 . 2))	2
(lib:nth 1 '(1 2))	2
(lib:nth 1 ())	:nil
(lib:nth 3 '(1 2))	:nil
(lib:nthcdr 0 '(1 . 2))	(1 . 2)
(lib:nthcdr 0 '(1 2))	(1 2)
(lib:nthcdr 0 ())	:nil
(lib:nthcdr 1 '(1 . 2))	2
(lib:nthcdr 1 '(1 2))	(2)
(lib:nthcdr 1 ())	:nil
(lib:nthcdr 3 '(1 2))	:nil
