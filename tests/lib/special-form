(lib:type-of (:lambda ()))	:func
(:quote a)	a
((:lambda ()))	:nil
((:lambda () 1))	1
((:lambda (a) a) 1)	1
((:lambda (a) (lib:sum a 2)) 1)	3
(:if :t :t :nil)	:t
(:if :nil :t :nil)	:nil
(:if 1 (lib:sum 1 0) (lib:sum 1 1))	1
(:if (lib:eq 1 1) (lib:sum 1 0) (lib:sum 1 1))	1
(:if (lib:eq 1 2) (lib:sum 1 0) (lib:sum 1 1))	2
