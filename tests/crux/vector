(crux:vector-len "abcde")	5
(crux:vector-len "abcdeghi")	8
(crux:vector-len "abcdegh")	7
(crux:vector-ref "abcde" 2)	#\c
(crux:vector-type "abcde")	:char
(crux:vector-len #(:t 1 a -3))	3
(crux:vector-ref #(:t 1 a -3) 1)	a
(crux:vector-type #(:byte 1 2 3))	:byte
(crux:vector-len #(:byte 1 2 3))	3
(crux:vector-ref #(:byte 1 2 3) 1)	2
(crux:vector-type #(:char #\1 #\2 #\3))	:char
(crux:vector-len #(:char #\1 #\2 #\3))	3
(crux:vector-ref #(:char #\1 #\2 #\3) 1)	#\2
(crux:vector-len #(:fixnum 1 2 -3))	3
(crux:vector-ref #(:fixnum 1 2 -3) 1)	2
(crux:vector-type #(:fixnum 1 2 -3))	:fixnum
(crux:vector-len #(:float 1.0 2.0 -3.0))	3
(crux:vector-ref #(:float 1.0 2.0 -3.0) 1)	2.0000
(crux:vector-type #(:float 1.0 2.0 -3.0))	:float
(crux:make-vector :byte '(1 2 3))	#(:byte 1 2 3)
(crux:make-vector :t '(1 2 3))	#(:t 1 2 3)
(crux:make-vector :fixnum '(1 2 3))	#(:fixnum 1 2 3)
(crux:make-vector :float '(1.0 2.0 -3.0))	#(:float 1.0000 2.0000 -3.0000)
