(crux:type-of (:lambda ()))	:func
(:quote a)	a
((:lambda ()))	:nil
((:lambda () 1))	1
((:lambda (a) a) 1)	1
((:lambda (a) (crux:sum a 2)) 1)	3
(:if :t :t :nil)	:t
(:if :nil :t :nil)	:nil
(:if 1 (crux:sum 1 0) (crux:sum 1 1))	1
(:if (crux:eq 1 1) (crux:sum 1 0) (crux:sum 1 1))	1
(:if (crux:eq 1 2) (crux:sum 1 0) (crux:sum 1 1))	2
